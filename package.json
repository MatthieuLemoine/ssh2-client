{
  "name": "ssh2-client",
  "version": "1.0.0",
  "description": "A Promise based wrapper around ssh2 to exec commands or setup a live shell on a remote host.",
  "main": "build/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel --presets es2015 -d ./build/lib lib/ && ./node_modules/.bin/babel --presets es2015 -o ./build/index.js index.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MatthieuLemoine/ssh2-client.git"
  },
  "keywords": [
    "ssh2",
    "ssh",
    "command",
    "exec",
    "shell",
    "live"
  ],
  "author": "MatthieuLemoine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MatthieuLemoine/ssh2-client/issues"
  },
  "homepage": "https://github.com/MatthieuLemoine/ssh2-client#readme",
  "dependencies": {
    "ssh2": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0"
  }
}
